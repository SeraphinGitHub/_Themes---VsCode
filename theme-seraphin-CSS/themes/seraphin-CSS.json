		"sideBarTitle.foreground": "#bbbbbb",
		"statusBarItem.remoteBackground": "#ff80ff",
		"statusBarItem.remoteForeground": "#ff0c0c",
		"tab.lastPinnedBorder": "#ff80ff",
	
	},
	"tokenColors": [
