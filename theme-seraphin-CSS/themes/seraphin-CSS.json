		"activityBarBadge.background": "#00b7ff",
		"editor.background": "#221A16",
		"editor.foreground": "#FFAA00",
		"editor.inactiveSelectionBackground": "#221A16",
		"editor.selectionHighlightBackground": "#47372F",
		"editorIndentGuide.activeBackground": "#6d6d6d",
